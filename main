------- new tab

local PlayerTab = Window:MakeTab({
	Name = "Player Things",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local Section = PlayerTab:AddSection({
	Name = "Player Propertys"
})

PlayerTab:AddSlider({
	Name = "WalkSpeed",
	Min = 16,
	Max = 500,
	Default = 16,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "WS",
	Callback = function(Value)
		game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = Value
	end    
})

PlayerTab:AddSlider({
	Name = "Jump Power",
	Min = 16,
	Max = 500,
	Default = 16,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "Height",
	Callback = function(Value)
		game.Players.LocalPlayer.Character.Humanoid.JumpPower = Value
	end    
})

PlayerTab:AddSlider({
	Name = "Spin Power",
	Min = 16,
	Max = 50000,
	Default = 16,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "Power",
	Callback = function(Value)
		power = Value
	end    
})

local Section = PlayerTab:AddSection({
	Name = "The Follow Things May Not Work"
})

local Section = TutTab:AddSection({
	Name = "Please Dont Report"
})

local Section = TutTab:AddSection({
	Name = "It Is Being Fixed!"
})

PlayerTab:AddTextbox({
    Name = "Teleport",
    Default = "User",
    TextDisappear = false,
    Callback = function(Value)
        local targetPlayer = game.Players:FindFirstChild(Value)
        if targetPlayer then
   player.Character.HumanoidRootPart.CFrame = targetPlayer.Character.HumanoidRootPart.CFrame 
        else
            OrionLib:MakeNotification({
                Name = "Invalid Username",
                Content = "Could not find player",
                Image = "rbxassetid://4483345998",
                Time = 3
            })
        end
    end
})

PlayerTab:AddButton({
	Name = "Reset Character",
	Callback = function()
      	player.Character.Humanoid.Health = 0
      	OrionLib:MakeNotification({
	Name = "Reset Success!",
	Content = "Reseted Character",
	Image = "rbxassetid://4483345998",
	Time = 2
})

  	end    
})
PlayerTab:AddButton({
	Name = "Sit",
	Callback = function()
      	player.Character.Humanoid.Sit = true
      	OrionLib:MakeNotification({
	Name = "Seated",
	Content = "Success",
	Image = "rbxassetid://4483345998",
	Time = 2
})
end
})
PlayerTab:AddToggle({
	Name = "Bhop",
	Default = false,
	Callback = function(Value)
	    if Value == true then
	        infinitejump = true
	  OrionLib:MakeNotification({
	Name = "Bhop Enabled",
	Content = "Enabled",
	Image = "rbxassetid://4483345998",
	Time = 2
	})
     	 while infinitejump == true do
 
      	    wait()
      	    player.Character.Humanoid.Jump = true
      	    end
      	else
      	    	  OrionLib:MakeNotification({
	Name = "Bhop Disabled",
	Content = "Disabled",
	Image = "rbxassetid://4483345998",
	Time = 2
	})
	infinitejump = false
  	 end
   end
})

local Section = PlayerTab:AddSection({
	Name = "Player Utils/Active Scripts"
})

PlayerTab:AddButton({
	Name = "Tp Tool",
	Callback = function()
        loadstring(game:HttpGet(("https://raw.githubusercontent.com/Bryan-Cracks/Bryan-Hub/refs/heads/main/tptool"),true))()
  	end    
})

PlayerTab:AddButton({
	Name = "Spin In The Selected Power",
	Callback = function()
        loadstring(game:HttpGet(("https://raw.githubusercontent.com/Bryan-Cracks/Bryan-Hub/refs/heads/main/spinbuttonsorce"),true))()
  	end    
})

